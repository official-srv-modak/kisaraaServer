<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>account</display-name>
  <servlet>
   	<servlet-name>intialise_session</servlet-name>
   	<servlet-class>com.kisara.Account.AccountManager</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>create_cust_account</servlet-name>
   	<servlet-class>com.kisara.Account.CreateAccount</servlet-class>
   </servlet>
    <servlet>
   	<servlet-name>mod_cust_account</servlet-name>
   	<servlet-class>com.kisara.Account.ModifyAccount</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>del_cust_account</servlet-name>
   	<servlet-class>com.kisara.Account.DeleteAccount</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>verify_info</servlet-name>
   	<servlet-class>com.kisara.Account.VerifyInfo</servlet-class>
   </servlet>
   <servlet-mapping>
   	<servlet-name>intialise_session</servlet-name>
   	<url-pattern>/login</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>create_cust_account</servlet-name>
   	<url-pattern>/createaccount</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>mod_cust_account</servlet-name>
   	<url-pattern>/modifyaccount</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>del_cust_account</servlet-name>
   	<url-pattern>/deleteaccount</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>verify_info</servlet-name>
   	<url-pattern>/verifyinfo</url-pattern>
   </servlet-mapping>
</web-app>